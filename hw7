#include <stdio.h>
#include <math.h>

int main() {
    double x, y;

    printf("Введите x: ");
    scanf("%lf", &x);

    if (x >= 5) {
        y = log10(2 * x) * log10(2 * x);
    } else if (x < -2) {
        y = 2 * x * x;
    } else {
        y = sin(x);
    }

    printf("Значение y = %.2f\n", y);

    return 0;
}
